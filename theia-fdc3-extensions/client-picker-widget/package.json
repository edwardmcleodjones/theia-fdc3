{
  "name": "client-picker-widget",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.0.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "1.65.1"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "ts-node": "^10.9.1",
    "ts-jest": "^26.5.6",
    "@types/node": "^24.6.0",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "^5.0.4",
    "globals": "^16.4.0",
    "typescript": "~5.4.5",
    "vite": "^6.3.5"
  },
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf lib",
    "build": "vite build && npm run dts",
    "watch": "vite build --watch --clearScreen false",
    "dts": "tsc --emitDeclarationOnly --declaration --outDir lib"
  },
  "scripts-old": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --config configs/jest.config.ts"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/client-picker-widget-frontend-module"
    }
  ]
}
