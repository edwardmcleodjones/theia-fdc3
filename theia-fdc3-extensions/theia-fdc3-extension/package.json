{
  "name": "theia-fdc3-extension",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.0.0",
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@finos/fdc3": "^2.2.0",
    "@theia/core": "1.65.1"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "typescript": "~5.4.5",
    "vite": "^6.3.5",
    "vite-plugin-lib-inject-css": "^2.2.2"
  },
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf lib",
    "build": "vite build && npm run dts",
    "watch": "vite build --watch --clearScreen false",
    "dts": "tsc --emitDeclarationOnly --declaration --outDir lib"
  },
  "scripts-old": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/theia-fdc3-extension-frontend-module"
    },
    {
      "frontend": "lib/browser/fdc3-frontend-module",
      "backend": "lib/node/fdc3-backend-module",
      "electron-browser": "lib/electron-browser/fdc3-preload"
    }
  ]
}
